{% extends "::base.html.twig" %}

{% block title "UCS Event Flow Analyser: All Events" %}

{% block body %}
    <nav>
        <h1>All events</h1>
        <ul>
            {% for event in events %}
                <li><a href="/events/event/{{ event|e }}">{{ event|e }}</a></li>
            {% endfor %}
        </ul>
    </nav>
{% endblock %}
